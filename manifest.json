{
  "manifest_version": 3,
  "name": "自动签收助手",
  "version": "1.0",
  "description": "自动点击页面上的'待签收件'按钮并完成签收流程",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}